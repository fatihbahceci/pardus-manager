<BorderPane prefHeight="306.0" prefWidth="326.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.pardus.manager.controls.UCMachineNameController">
   <top>
